{
  "author": "<aperos.dev@gmail.com>",
  "description": "Useful TypeScript classes suitable for both browser and Node.js",
  "devDependencies": {
    "dts-bundle": "latest",
    "make-dts-bundle": "latest",
    "rollup": "latest",
    "rollup-plugin-terser": "latest",
    "rollup-plugin-typescript2": "latest",
    "ts-node": "latest",
    "typescript": "latest"
  },
  "license": "MIT",
  "name": "@aperos/ts-goodies",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c && npm run build-dts",
    "build-dts": "make-dts-bundle -b build",
    "build-production": "BUILD=production npm run build",
    "clean": "rm -rf build dist",
    "prepare": "npm run clean && npm run build-production"
  },
  "types": "dist/index.d.ts",
  "version": "0.0.1"
}
